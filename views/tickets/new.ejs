<%- include('../partials/header.ejs') %>

<form action="/flights/<%= flightId %>/tickets" method="POST" id="ticket-create">
    <label>Seat:
            <input type="text" name="seat" required pattern="[A-F][1-9]\d?">
    </label><br>
    <label>Price:
            <input type="number" name="price">
    </label><br>
<br>
<button type="submit" form="ticket-create">Create Ticket</button>
</form>

